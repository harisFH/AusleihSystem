        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	var grEquipment = new GlideRecord("x_sigh_ausleihsyst_equipment");
	
	if (grEquipment.get(current.equipment)){
		if(grEquipment.amount<current.anzahl){
			gs.addInfoMessage('Sie können nicht mehr Produkte Ausleihen wie zur Verfügung stehen');
			current.setAbortAction(true);
		}
		else if (current.anzahl<=0){
			gs.addInfoMessage('Digga du willst 0 Produkte oder weniger Ausleihen, Dein Ernst? ');
			current.setAbortAction(true);
		}
	}

})(current, previous);]]></script>

        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	/*besorge mir die Records vom cart mit der OrderID
	zähle die Anzahl an die entsprechende Produkte zurück
	lösche Cart einträge lösche Order eintrag*/
	var grCart = new GlideRecord("x_sigh_ausleihsyst_m2m_equipments_orders");
	var grEquip = new GlideRecord("x_sigh_ausleihsyst_equipment");
	if (grCart.get("order",current.sys_id)){
		while (grCart.next()){
			if (grEquip.get("sys_id",grCart.equipment)){
				var Summe = grEquip.amount+ grCart.anzahl;
				grEquip.amount=Summe;
			}
		}
		grCart.deleteMultiple();
		current.deleteRecord();
		gs.addInfoMessage("Die Rückgabe ist erfolgreich im System registriert");
	}

})(current, previous);]]></script>
